mode "interactive" {
#bindsym $mod+Return mode "normal"
#bindsym $mod+Escape mode "normal"
# 133 = SuperL = mod4
bindcode 133 mode "normal"
# Use fn volumekeys
bindsym XF86AudioMute exec amixer -D pulse set 'Master' 1+ toggle
bindsym XF86AudioLowerVolume exec amixer sset 'Master' 5%-
bindsym XF86AudioRaiseVolume exec amixer sset 'Master' 5%+

# Use fn brightness control
bindsym XF86MonBrightnessUp exec light -A 10 # increase brightness
#bindsym XF86MonBrightnessDown exec light -U 10 # decrease brightness
bindsym XF86MonBrightnessDown exec light -S $(echo "$(light -G) / 2" | bc -l) # decrease lightness by halve
# vimlike marks #TODO
#bindsym m i3-input -F 'mark --toggle %s' -l 1 -P 'Mark: '
#bindsym M i3-input -F '[con_mark="%s"] focus' -l 1 -P 'Go to: '

# change focus
bindsym $mod+h focus left, mode "normal"
bindsym $mod+j focus down, mode "normal"
bindsym $mod+k focus up, mode "normal"
bindsym $mod+l focus right, mode "normal"

# move focused window
bindsym $mod+Shift+h move left, mode "normal"
bindsym $mod+Shift+j move down, mode "normal"
bindsym $mod+Shift+k move up, mode "normal"
bindsym $mod+Shift+l move right, mode "normal"

# switch workspace (gridbased)
bindsym $mod+b workspace   prev, mode "normal"
#bindsym $mod+n workspace  next, mode "normal"
#bindsym $mod+m workspace  prev, mode "normal"
bindsym $mod+comma workspace   next, mode "normal"
bindsym $mod+Tab workspace back_and_forth, mode "normal"

# move focused window to workspace (gridbased)
bindsym $mod+Shift+b   move container to workspace prev, mode "normal"
#bindsym $mod+Shift+n  move container to workspace next, mode "normal"
#bindsym $mod+Shift+m  move container to workspace prev, mode "normal"
bindsym $mod+Shift+comma   move container to workspace next, mode "normal"
bindsym $mod+Shift+Tab move container to workspace back_and_forth, mode "normal"

# switch to workspace
bindsym $mod+1 workspace $ws1, mode "normal"
bindsym $mod+2 workspace $ws2, mode "normal"
bindsym $mod+3 workspace $ws3, mode "normal"
bindsym $mod+4 workspace $ws4, mode "normal"
bindsym $mod+5 workspace $ws5, mode "normal"
bindsym $mod+6 workspace $ws6, mode "normal"
bindsym $mod+7 workspace $ws7, mode "normal"
bindsym $mod+8 workspace $ws8, mode "normal"
bindsym $mod+9 workspace $ws9, mode "normal"
bindsym $mod+0 workspace $ws10, mode "normal"

# move focused container to workspace
bindsym $mod+Shift+1 move container to workspace $ws1, mode "normal"
bindsym $mod+Shift+2 move container to workspace $ws2, mode "normal"
bindsym $mod+Shift+3 move container to workspace $ws3, mode "normal"
bindsym $mod+Shift+4 move container to workspace $ws4, mode "normal"
bindsym $mod+Shift+5 move container to workspace $ws5, mode "normal"
bindsym $mod+Shift+6 move container to workspace $ws6, mode "normal"
bindsym $mod+Shift+7 move container to workspace $ws7, mode "normal"
bindsym $mod+Shift+8 move container to workspace $ws8, mode "normal"
bindsym $mod+Shift+9 move container to workspace $ws9, mode "normal"
bindsym $mod+Shift+0 move container to workspace $ws10, mode "normal"
}

mode "normal" {
##Internal stuff
# remake and reload the configuration file
bindsym $mod+Shift+c exec --no-startup-id cat ~/.config/i3/config.d/* > ~/.config/i3/config; exec --no-startup-id i3-migrate-config-to-v4 ~/.config/i3/config ~/.config/i3/config; exec --no-startup-id reseti3; reload

# restart i3 inplace (preserves your layout/session, can be used to upgrade i3)
# bindsym $mod+Shift+r restart, exec --no-startup-id xdotool key --clearmodifiers n
# bindsym $mod+Shift+r restart; mode $mod+Shift+n
bindsym $mod+Shift+r restart
bindsym $mod+Shift+n mode "normal"

# floating quick use xprop
bindsym Print exec $term --class floatingterm -x "pipe 'xprop -len 60 -spy' 'less'"

## i3 related content
# start a terminal
bindsym o exec $term; mode "interactive"

# move the (Tilda-like but terminator) scratchpad terminal to or from the scratchpad
bindsym grave scratchpad show; mode "interactive"

# start dmenu (a program launcher)
bindsym d exec dmenu_run

#TODO # yank current selection maybe to scratchpad? or maybe as a textcopy
# bindsym y

# execute a bash command
set $bcmd "exec $term --class floatingterm -xh '%s'"
bindsym Shift+semicolon exec --no-startup-id i3-input -F $bcmd -P ":"
#bindsym Shift+semicolon mode "interactive", exec --no-startup-id i3-input -F $bcmd -P ":"

# search and represent
set $gcmd "exec $term --class floatingterm -x \\\"pipe 'grep' '%s'\\\""
bindsym slash mode exec --no-startup-id i3-input -F $gcmd -P "/"
#bindsym slash mode "interactive", exec --no-startup-id i3-input -F $gcmd -P "/"
# Use fn volumekeys
bindsym XF86AudioMute exec amixer -D pulse set 'Master' 1+ toggle
bindsym XF86AudioLowerVolume exec amixer sset 'Master' 5%-
bindsym XF86AudioRaiseVolume exec amixer sset 'Master' 5%+

# Use fn brightness control
bindsym XF86MonBrightnessUp exec light -A 10 # increase brightness
#bindsym XF86MonBrightnessDown exec light -U 10 # decrease brightness
bindsym XF86MonBrightnessDown exec light -S $(echo "$(light -G) / 2" | bc -l) # decrease lightness by halve
# define mode keys
bindsym i mode "interactive"
bindsym v mode "visual"
bindsym r mode "resize"

bindsym z mode "z"
bindsym Z mode "z"

bindsym q mode "$mode_system"
#bindsym $mod+Shift+e exec "i3-nagbar -t warning -m 'You pressed the exit shortcut. Do you really want to exit i3? This will end your X session.' -b 'Yes, exit i3' 'i3-msg exit'"
# vimlike marks #TODO
#bindsym m i3-input -F 'mark --toggle %s' -l 1 -P 'Mark: '
#bindsym M i3-input -F '[con_mark="%s"] focus' -l 1 -P 'Go to: '

# change focus
bindsym h focus left
bindsym j focus down
bindsym k focus up
bindsym l focus right

# move focused window
bindsym Shift+h move left
bindsym Shift+j move down
bindsym Shift+k move up
bindsym Shift+l move right

# switch workspace (gridbased)
bindsym b workspace   prev
#bindsym n workspace  next
#bindsym m workspace  prev
bindsym comma workspace   next
bindsym Tab workspace back_and_forth

# move focused window to workspace (gridbased)
bindsym Shift+b   move container to workspace prev
#bindsym Shift+n  move container to workspace next
#bindsym Shift+m  move container to workspace prev
bindsym Shift+comma   move container to workspace next
bindsym Shift+Tab move container to workspace back_and_forth

# switch to workspace
bindsym 1 workspace $ws1
bindsym 2 workspace $ws2
bindsym 3 workspace $ws3
bindsym 4 workspace $ws4
bindsym 5 workspace $ws5
bindsym 6 workspace $ws6
bindsym 7 workspace $ws7
bindsym 8 workspace $ws8
bindsym 9 workspace $ws9
bindsym 0 workspace $ws10

# move focused container to workspace
bindsym Shift+1 move container to workspace $ws1
bindsym Shift+2 move container to workspace $ws2
bindsym Shift+3 move container to workspace $ws3
bindsym Shift+4 move container to workspace $ws4
bindsym Shift+5 move container to workspace $ws5
bindsym Shift+6 move container to workspace $ws6
bindsym Shift+7 move container to workspace $ws7
bindsym Shift+8 move container to workspace $ws8
bindsym Shift+9 move container to workspace $ws9
bindsym Shift+0 move container to workspace $ws10
# vimlike marks #TODO
#bindsym m i3-input -F 'mark --toggle %s' -l 1 -P 'Mark: '
#bindsym M i3-input -F '[con_mark="%s"] focus' -l 1 -P 'Go to: '

# change focus
bindsym $mod+h focus left, mode "normal"
bindsym $mod+j focus down, mode "normal"
bindsym $mod+k focus up, mode "normal"
bindsym $mod+l focus right, mode "normal"

# move focused window
bindsym $mod+Shift+h move left, mode "normal"
bindsym $mod+Shift+j move down, mode "normal"
bindsym $mod+Shift+k move up, mode "normal"
bindsym $mod+Shift+l move right, mode "normal"

# switch workspace (gridbased)
bindsym $mod+b workspace   prev, mode "normal"
#bindsym $mod+n workspace  next, mode "normal"
#bindsym $mod+m workspace  prev, mode "normal"
bindsym $mod+comma workspace   next, mode "normal"
bindsym $mod+Tab workspace back_and_forth, mode "normal"

# move focused window to workspace (gridbased)
bindsym $mod+Shift+b   move container to workspace prev, mode "normal"
#bindsym $mod+Shift+n  move container to workspace next, mode "normal"
#bindsym $mod+Shift+m  move container to workspace prev, mode "normal"
bindsym $mod+Shift+comma   move container to workspace next, mode "normal"
bindsym $mod+Shift+Tab move container to workspace back_and_forth, mode "normal"

# switch to workspace
bindsym $mod+1 workspace $ws1, mode "normal"
bindsym $mod+2 workspace $ws2, mode "normal"
bindsym $mod+3 workspace $ws3, mode "normal"
bindsym $mod+4 workspace $ws4, mode "normal"
bindsym $mod+5 workspace $ws5, mode "normal"
bindsym $mod+6 workspace $ws6, mode "normal"
