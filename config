# move focused container to workspace
bindsym $mod+Shift+1 move container to workspace $ws1, mode "normal"
bindsym $mod+Shift+2 move container to workspace $ws2, mode "normal"
bindsym $mod+Shift+3 move container to workspace $ws3, mode "normal"
bindsym $mod+Shift+4 move container to workspace $ws4, mode "normal"
bindsym $mod+Shift+5 move container to workspace $ws5, mode "normal"
bindsym $mod+Shift+6 move container to workspace $ws6, mode "normal"
bindsym $mod+Shift+7 move container to workspace $ws7, mode "normal"
bindsym $mod+Shift+8 move container to workspace $ws8, mode "normal"
bindsym $mod+Shift+9 move container to workspace $ws9, mode "normal"
bindsym $mod+Shift+0 move container to workspace $ws10, mode "normal"
# split in horizontal orientation
#bindsym | split h
bindsym Shift+backslash split h

# split in vertical orientation
bindsym Shift+minus split v
#bindsym _ split v

# container layout
bindsym f fullscreen toggle
bindsym t layout tabbed
bindsym w layout toggle split

# focus the parent container
bindsym a focus parent

# focus the child container
bindsym x focus child

# These two can keep the $mod, since I won't be using them often
# toggle tiling / floating
bindsym $mod+Shift+space floating toggle

# change focus between tiling / floating windows
bindsym $mod+space focus mode_toggle

# Use Mouse+$mod to drag floating windows to their wanted position NOT currently in use
# floating_modifier button1
# for i in {8..255}; do echo -e "bindcode $i mode \"normal\""; done > 38return_nonkeybinds

bindcode 8 nop
